@model List<Models.HarenViewModel>

<h1>Lista de Harens</h1>

<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Número Macho</th>
            <th>Número Fêmea</th>
            <th>Status</th>
            <th>Ações</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var haren in Model)
        {
            <tr>
                <td>@haren.Id</td>
                <td>@haren.NumeroMacho</td>
                <td>@haren.NumeroFemea</td>
                <td>@haren.Status</td>
                <td>
                    <a asp-action="Details" asp-route-id="@haren.Id">Detalhes</a> |
                    <a asp-action="Edit" asp-route-id="@haren.Id">Editar</a> |
                    <a asp-action="Delete" asp-route-id="@haren.Id">Excluir</a>
                </td>
            </tr>
        }
    </tbody>
</table>

<a asp-action="Create">Adicionar Novo Haren</a>
